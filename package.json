{
  "name": "gc-stats",
  "version": "1.1.1",
  "description": "Exposes node v8 garbage collection stats",
  "main": "index.js",
  "repository": "git@github.com:dainis/node-gcstats.git",
  "license": "Unlicense",
  "engines": {
    "node": ">=0.12"
  },
  "scripts": {
    "install": "node-pre-gyp install --fallback-to-build",
    "test": "mocha --expose-gc tests",
    "example": "node --expose-gc example.js",
    "prepublish": "npm ls",
    "retire": "retire --node --package --verbose --nocache --severity high"
  },
  "dependencies": {
    "nan": "^2.6.2",
    "node-pre-gyp": "^0.10.0"
  },
  "gypfile": true,
  "devDependencies": {
    "aws-sdk": "^2.75.0",
    "mocha": "^2.5.3",
    "node-gyp": "^3.6.2",
    "semver": "^5.3.0",
    "should": "^7.1.0",
    "retire": "^1.6.0"
  },
  "bundledDependencies": [
    "node-pre-gyp"
  ],
  "binary": {
    "module_name": "gcstats",
    "module_path": "./build/{module_name}/v{version}/{configuration}/{node_abi}-{platform}-{arch}/",
    "remote_path": "./{module_name}/v{version}/{configuration}/",
    "package_name": "{node_abi}-{platform}-{arch}.tar.gz",
    "host": "https://node-binaries.s3.amazonaws.com"
  }
}
